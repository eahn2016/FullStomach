void pause() {
    waitForVBlank();

    if ((!(~(oldButtons)&((1<<7))) && (~buttons & ((1<<7)))) && pauseState != 2) {
        pauseState++;
        shadowOAM[1].attr0 += 15;
        DMANow(3, shadowOAM, ((OBJ_ATTR*)(0x7000000)), 128 * 4);
    }
    if ((!(~(oldButtons)&((1<<6))) && (~buttons & ((1<<6)))) && pauseState != 0) {
        pauseState--;
        shadowOAM[1].attr0 -= 15;
        DMANow(3, shadowOAM, ((OBJ_ATTR*)(0x7000000)), 128 * 4);
    }
    if ((!(~(oldButtons)&((1<<3))) && (~buttons & ((1<<3))))) {
        if (pauseState == 0) {
            goToGame();
        } else if (pauseState == 1) {
            if (cheat) {
                cheat = 0;
            } else {
                cheat = 1;
            }
        } else if (pauseState == 2) {
            pauseState = 0;
            shadowOAM[1].attr0 = 51;
            textSeed = 0;
            level = 0;
            firstSwitch = 0;
            cheat = 0;
            player.worldRow = 111;
