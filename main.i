    loadPalette(titleScreenPal);
    DMANow(3, titleScreenTiles, &((charblock *)0x6000000)[1], 6016 / 2);
    DMANow(3, titleScreenMap, &((screenblock *)0x6000000)[29], 2048 / 2);

    DMANow(3, spritesheet2Pal, ((unsigned short *)0x5000200), 256);
    DMANow(3, spritesheet2Tiles, &((charblock *)0x6000000)[4], 32768);
    hideSprites();
    DMANow(3, shadowOAM, ((OBJ_ATTR*)(0x7000000)), 128 * 4);

    (*(unsigned short *)0x4000000) = 0 | (1<<8) | (1<<12);
    (*(volatile unsigned short*)0x4000008) = (0<<14) | ((1)<<2) | ((29)<<8);

    shadowOAM[1].attr0 = 110 | (0<<13) | (1<<14);
    shadowOAM[1].attr1 = 80 | (1<<14);
    shadowOAM[1].attr2 = ((1)<<12) | ((1)*32+(0));
    DMANow(3, shadowOAM, ((OBJ_ATTR*)(0x7000000)), 128 * 4);

    (*(volatile unsigned short *)0x04000010) = 0;
    (*(volatile unsigned short *)0x04000012) = 0;

    state = START;
}

void start() {
    waitForVBlank();

    if ((!(~(oldButtons)&((1<<7))) && (~buttons & ((1<<7)))) && titleState != 1) {
        pauseState = 1;
        shadowOAM[1].attr0 += 12;
        shadowOAM[1].attr1 -= 20;
        titleState++;
        DMANow(3, shadowOAM, ((OBJ_ATTR*)(0x7000000)), 128 * 4);
    }
    if ((!(~(oldButtons)&((1<<6))) && (~buttons & ((1<<6)))) && titleState != 0) {
        pauseState = 0;
        shadowOAM[1].attr0 -= 12;
        shadowOAM[1].attr1 += 20;
        titleState--;
        DMANow(3, shadowOAM, ((OBJ_ATTR*)(0x7000000)), 128 * 4);
    }

    if ((!(~(oldButtons)&((1<<3))) && (~buttons & ((1<<3))))) {
        if (titleState == 0) {
            goToCutscene();
            initCutscene();
        } else if (titleState == 1) {
            goToInstructions();
        }
    }
}

